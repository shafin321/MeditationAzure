

@model IEnumerable<MeiditationActivity.Models.ApplicationUser>



<h2>Users</h2>

<table class="table table-bordered table-hover">

    <thead>
        <tr>
            <th>User</th>
            <th>Created</th>
        </tr>

    </thead>

    @{ foreach (var user in Model)

        {
            <tr>
                <td> <a asp-action="Detail" asp-controller="ApplicationUser" asp-route-id="@user.Id"> @user.UserName></a></td>

                <td>@user.MemberSince</td>
            </tr>


        }

    }
</table>


